{
  "name": "resource-machine",
  "description": "ResourceMachine - WebMachine for NodeJS",
  "version": "0.1.0",
  "homepage": "https://github.com/JonGretar/ResourceMachine",
  "author": {
    "name": "Jon Gretar Borgthorsson",
    "email": "jongretar@jongretar.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/JonGretar/ResourceMachine.git"
  },
  "bugs": {
    "url": "https://github.com/JonGretar/ResourceMachine/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/JonGretar/ResourceMachine/blob/master/LICENSE"
    }
  ],
  "main": "lib/index",
  "engines": {
    "node": ">= 0.11.7",
    "npm": ">=1.4.3"
  },
  "scripts": {
    "test": "npm run-script mocha",
    "mocha": "./node_modules/.bin/mocha --harmony -R spec 'test/**/*test.js'",
    "example": "node --harmony examples/demo/server.js | ./node_modules/.bin/bunyan",
    "example-watch": "nodemon --harmony examples/demo/server.js | ./node_modules/.bin/bunyan",
    "doc": "./node_modules/.bin/gitbook serve ./doc",
    "doc-build": "./node_modules/.bin/gitbook ./doc"
  },
  "dependencies": {
    "suspend": "~0.5.0",
    "verror": "^1.4.0",
    "bunyan": "~0.22.2",
    "extend": "^1.2.1",
    "negotiator": "^0.4.2",
    "mime": "~1.2.11"
  },
  "devDependencies": {
    "mocha": "~1.10.0",
    "chai": "~1.8.0",
    "gulp": "~3.6.0",
    "gulp-jshint": "~1.3.4",
    "gulp-mocha": "~0.4.1",
    "jshint-stylish": "~0.1.5",
    "supertest": "~0.10.0",
    "gitbook": "~0.4.6",
    "JSONStream": "~0.7.2"
  },
  "keywords": [
    "web",
    "framework",
    "http",
    "webmachine"
  ]
}
